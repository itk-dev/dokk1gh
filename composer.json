{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-soap": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "itk-dev/config-bundle": "^3.0",
        "itk-dev/database-bundle": "^1.3",
        "itk-dev/monolog-db-bundle": "dev-develop",
        "itk-dev/user-bundle": "2.x-dev",
        "jms/serializer-bundle": "^3.7",
        "nelmio/api-doc-bundle": "~3.3.1",
        "sensio/framework-extra-bundle": "^5.6",
        "stof/doctrine-extensions-bundle": "^1.5",
        "suncat/mobile-detect-bundle": "^1.1",
        "superbrave/gdpr-bundle": "^2.2",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/options-resolver": "4.4.*",
        "symfony/twig-bundle": "^4.4",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "4.4.*",
        "symfonycasts/reset-password-bundle": "^1.1"
    },
    "require-dev": {
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/symfony-extension": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "friendsoftwig/twigcs": "^4.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/maker-bundle": "^1.23",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@install-git-hooks"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "coding-standards-check/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix --dry-run"
        ],
        "coding-standards-check/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "coding-standards-check/twigcs": [
            "vendor/bin/twigcs templates/"
        ],
        "coding-standards-check": [
            "@coding-standards-check/php-cs-fixer",
            "@coding-standards-check/phpcs"
        ],
        "coding-standards-apply/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix"
        ],
        "coding-standards-apply/phpcs": [
            "vendor/bin/phpcbf --standard=phpcs.xml.dist"
        ],
        "coding-standards-apply": [
            "@coding-standards-apply/php-cs-fixer",
            "@coding-standards-apply/phpcs"
        ],
        "install-git-hooks": [
            "ln -sf ../../githooks/pre-commit .git/hooks"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    },
    "repositories": {
        "itk-dev/monolog-db-bundle": {
            "type": "path",
            "url": "packages/itk-dev/monolog-db-bundle"
        }
    }
}
